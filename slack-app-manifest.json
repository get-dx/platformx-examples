{
  "display_information": {
    "name": "My Bot"
  },
  "features": {
    "bot_user": {
      "display_name": "My Bot",
      "always_online": true
    }
  },
  "oauth_config": {
    "scopes": {
      "bot": [
        "chat:write",
        "groups:write",
        "mpim:write"
      ]
    }
  },
  "settings": {
    "interactivity": {
      "is_enabled": true,
      "request_url": "https://app.getdx.com/slack/interaction"
    },
    "org_deploy_enabled": true,
    "socket_mode_enabled": false,
    "token_rotation_enabled": false
  }
}